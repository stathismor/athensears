# Strapi CMS (shared with gig-crawler)
STRAPI_API_URL=http://localhost:1337
STRAPI_API_TOKEN=your_strapi_token_here

# Brave Web Search API (can reuse same key)
BRAVE_API_KEY=your_brave_api_key_here

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash-latest

# Server
PORT=3001
NODE_ENV=development

# Cron (same schedule as gig-crawler: 2 AM Athens time)
CRON_SCHEDULE=0 2 * * *
TZ=Europe/Athens

# Logging
LOG_LEVEL=info
