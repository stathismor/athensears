{
  "name": "gig-crawler-2",
  "version": "2.0.0",
  "private": true,
  "description": "Two-pass Athens gig crawler with Gemini LLM",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "dev:docker": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node --env-file=.env dist/index.js",
    "start:docker": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "crawler",
    "gigs",
    "athens",
    "music",
    "gemini",
    "two-pass"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@mozilla/readability": "^0.5.0",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "express": "^4.21.2",
    "jsdom": "^25.0.1",
    "node-cron": "^3.0.3",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
