{
  "kind": "collectionType",
  "collectionName": "gigs",
  "info": {
    "singularName": "gig",
    "pluralName": "gigs",
    "displayName": "Gig",
    "description": "A live music event"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "time_display": {
      "type": "string",
      "maxLength": 20
    },
    "price": {
      "type": "string",
      "maxLength": 50
    },
    "description": {
      "type": "text"
    },
    "venue": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::venue.venue"
    }
  }
}
